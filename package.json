{
  "name": "cyberloop",
  "version": "1.0.0",
  "description": "Artificial Intelligence Control Loop",
  "keywords": [
    "AI",
    "AICL",
    "cybernetic",
    "control",
    "theory"
  ],
  "homepage": "https://github.com/roackb2/cyberloop#readme",
  "bugs": {
    "url": "https://github.com/roackb2/cyberloop/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roackb2/cyberloop.git"
  },
  "license": "Apache-2.0",
  "author": "Jay / Fienna Liang (roackb2@gmail.com)",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:cov": "vitest --coverage",
    "test:ui": "vitest --ui",
    "examples:quickstart": "tsx src/examples/quickstart.ts",
    "examples:github": "tsx src/examples/github-query-demo.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.7.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
